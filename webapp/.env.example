# Authentication Configuration
# Copy this file to .env and configure your settings

# Password hash (generate using setup_password.py)
# DO NOT use this example hash in production!
APP_PASSWORD_HASH=your_password_hash_here

# Session secret key (generate using setup_password.py)
# This should be a random, secure string
SECRET_KEY=your_secret_key_here

# Session lifetime in seconds (default: 86400 = 24 hours)
SESSION_LIFETIME=86400

# Environment (development or production)
ENVIRONMENT=production
